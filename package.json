    "@mantine/form": "^4.2.12",
    "@mantine/hooks": "^4.2.12",
    "@mantine/modals": "^4.2.12",
    "@mantine/notifications": "^4.2.12",
    "@mantine/prism": "^4.2.12",
    "@mantine/rte": "^4.2.12",
    "@mantine/spotlight": "^4.2.12",
    "@mantine/ssr": "^4.2.12",
    "@prisma/client": "^4.0.0",
    "@remix-run/node": "^1.6.4",
    "@remix-run/react": "^1.6.4",
    "@remix-run/vercel": "^1.6.4",
    "@vercel/node": "^1.15.2",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.3",
