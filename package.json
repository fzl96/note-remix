    "@types/react-dom": "^17.0.17",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "eslint": "^8.15.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
